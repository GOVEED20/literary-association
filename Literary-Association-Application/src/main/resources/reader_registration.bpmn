              <camunda:property id="type" value="text" />
            </camunda:properties>
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="email" label="Email" type="string">
            <camunda:properties>
              <camunda:property id="type" value="text" />
              <camunda:property id="pattern" value="^([a-zA-Z0-9_\-\.]+)@([a-zA-Z0-9_\-\.]+)\.([a-zA-Z]{2,5})$" />
            </camunda:properties>
            <camunda:validation>
              <camunda:constraint name="required" />
